<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %} - Gallery Share
		</title>
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			<link rel="stylesheet" href="{{ asset('assets/profile.css') }}">
			<link rel="stylesheet" href="{{ asset('assets/general.css') }}">
			<link rel="stylesheet" href="{{ asset('assets/main.css') }}">
			<link rel="stylesheet" href="{{ asset('assets/admin.css') }}">
			<link rel="stylesheet" href="{{ asset('assets/category.css') }}">
			<link rel="stylesheet" href="{{ asset('assets/publicationcat.css') }}">
			<link rel="stylesheet" href="{{ asset('assets/publicreate.css') }}">
			<link rel="stylesheet" href="{{ asset('assets/publicationview.css') }}">
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% include "_partials/nav.html.twig" %}
		{% block body %}{% endblock %}
	</body>
</html>
